<p id="notice"><%= notice %></p>

<h1>Listing Car Products</h1>

<table>
  <thead>
    <tr>
      <th>Product</th>
      <th>Cart</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @car_products.each do |car_product| %>
      <tr>
        <td><%= car_product.product %></td>
        <td><%= car_product.cart %></td>
        <td><%= link_to 'Show', car_product %></td>
        <td><%= link_to 'Edit', edit_car_product_path(car_product) %></td>
        <td><%= link_to 'Destroy', car_product, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car product', new_car_product_path %>

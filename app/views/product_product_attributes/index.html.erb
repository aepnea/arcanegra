<p id="notice"><%= notice %></p>

<h1>Listing Product Product Attributes</h1>

<table>
  <thead>
    <tr>
      <th>Product</th>
      <th>Product attribute</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_product_attributes.each do |product_product_attribute| %>
      <tr>
        <td><%= product_product_attribute.product_id %></td>
        <td><%= product_product_attribute.product_attribute_id %></td>
        <td><%= link_to 'Show', product_product_attribute %></td>
        <td><%= link_to 'Edit', edit_product_product_attribute_path(product_product_attribute) %></td>
        <td><%= link_to 'Destroy', product_product_attribute, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product product attribute', new_product_product_attribute_path %>

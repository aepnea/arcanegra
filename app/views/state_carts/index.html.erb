<p id="notice"><%= notice %></p>

<h1>Listing State Carts</h1>

<table>
  <thead>
    <tr>
      <th>State</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @state_carts.each do |state_cart| %>
      <tr>
        <td><%= state_cart.state %></td>
        <td><%= link_to 'Show', state_cart %></td>
        <td><%= link_to 'Edit', edit_state_cart_path(state_cart) %></td>
        <td><%= link_to 'Destroy', state_cart, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New State cart', new_state_cart_path %>
